pipeline {
    agent any

    stages {
        stage('Sum') {
            steps {
                echo '2+3 = 5'
            }
        }
        stage('Sub') {
            steps {
                echo '3-2 = 1'
            }
        }
        stage('Div') {
            steps {
                echo '6/2 = 3'
            }
        }
    }
}